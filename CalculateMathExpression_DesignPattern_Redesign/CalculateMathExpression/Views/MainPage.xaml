                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />


                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Button
                            Grid.Row="0"
                            Grid.Column="0"
                            Width="Auto"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔX Camera 1"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[XC1]}"
                            Tag="[ΔXC1]" />
                        <Button
                            Grid.Row="0"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔY Camera 1"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[YC1]}"
                            Tag="[ΔYC1]" />
                        <Button
                            Grid.Row="1"
                            Grid.Column="0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔX Camera 2"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[XC2]}"
                            Tag="[ΔXC2]" />
                        <Button
                            Grid.Row="1"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔY Camera 2"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[YC2]}"
                            Tag="[ΔYC2]" />
                        <Button
                            Grid.Row="2"
                            Grid.Column="0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔX Camera 3"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[XC3]}"
                            Tag="[ΔXC3]" />
                        <Button
                            Grid.Row="2"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔY Camera 3"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[YC3]}"
                            Tag="[ΔYC3]" />
                        <Button
                            Grid.Row="3"
                            Grid.Column="0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔX Camera 4"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[XC4]}"
                            Tag="[ΔXC4]" />
                        <Button
                            Grid.Row="3"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔY Camera 4"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[YC4]}"
                            Tag="[ΔYC4]" />
                        <Button
                            Grid.Row="4"
                            Grid.Column="0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Content}"
                            Content="."
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[DOT]}"
                            Tag="DOT" />

                    </Grid>
                    <Grid
                        Grid.Row="0"
                        Grid.Column="2"
                        Margin="5,0"
                        ColumnSpacing="5"
                        RowSpacing="5">

                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />


                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Button
                            Grid.Row="0"
                            Grid.Column="0"
                            Width="Auto"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔX Bot. Camera 1"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[XBC1]}"
                            Tag="[ΔXBC1]" />
                        <Button
                            Grid.Row="0"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔY Bot. Camera 1"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[YBC1]}"
                            Tag="[ΔYBC1]" />
                        <Button
                            Grid.Row="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔX Bot. Camera 2"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[XBC2]}"
                            Tag="[ΔXBC2]" />
                        <Button
                            Grid.Row="1"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔY Bot. Camera 2"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[YBC2]}"
                            Tag="[ΔYBC2]" />
                        <Button
                            Grid.Row="2"
                            Grid.Column="0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔX Bot. Camera 3"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[XBC3]}"
                            Tag="[ΔXBC3]" />
                        <Button
                            Grid.Row="2"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔY Bot. Camera 3"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[YBC3]}"
                            Tag="[ΔYBC3]" />
                        <Button
                            Grid.Row="3"
                            Grid.Column="0"
                            Width="Auto"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔX Bot. Camera 4"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[XBC4]}"
                            Tag="[ΔXBC4]" />
                        <Button
                            Grid.Row="3"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                            Content="ΔY Bot. Camera 4"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[YBC4]}"
                            Tag="[ΔYBC4]" />

                    </Grid>


                </Grid>
                <StackPanel
                    Grid.Row="5"
                    Margin="0,30,0,30"
                    VerticalAlignment="Bottom">
                    <Button
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Command="{Binding Path=Main.SaveCommand}"
                        Content="Save" />
                </StackPanel>
            </Grid>
        </PivotItem>
        <PivotItem Header="Calculate">
            <Grid Margin="12,32,0,0">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" MinWidth="600" />
                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" MinHeight="200" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Grid Grid.Row="0" Grid.Column="0">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>

                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="0"
                        Width="Auto"
                        Height="Auto"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center">
                        SpreadX
                    </TextBlock>
                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center">
                        SpreadY
                    </TextBlock>
                    <TextBox
                        Name="XMathExpressionTextBox"
                        Grid.Row="0"
                        Grid.Column="1"
                        Margin="4"
                        IsReadOnly="True"
                        Text="{Binding Path=SecondTabViewModel.SavedXFormula, Mode=OneWay}" />
                    <TextBox
                        Name="YMathExpressionTextBox"
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="4"
                        IsReadOnly="True"
                        Text="{Binding Path=SecondTabViewModel.SavedYFormula, Mode=OneWay}" />
                </Grid>
