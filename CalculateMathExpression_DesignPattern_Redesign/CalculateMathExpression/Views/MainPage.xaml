                        Grid.Row="3"
                        Grid.Column="0"
                        Header="ΔXC4:"
                        InputScope="Number"
                        Tag="[ΔXC4]"
                        Text="{Binding Path=SecondTabViewModel.XC4Box.Value, Mode=TwoWay}" />
                    <TextBox
                        Name="yc1"
                        Grid.Row="0"
                        Grid.Column="1"
                        Header="ΔYC1:"
                        InputScope="NumericPin"
                        Tag="[ΔYC1]"
                        Text="{Binding Path=SecondTabViewModel.YC1Box.Value, Mode=TwoWay}" />
                    <TextBox
                        Name="yc2"
                        Grid.Row="1"
                        Grid.Column="1"
                        Header="ΔYC2:"
                        InputScope="Number"
                        Tag="[ΔYC2]"
                        Text="{Binding Path=SecondTabViewModel.YC2Box.Value, Mode=TwoWay}" />
                    <TextBox
                        Name="yc3"
                        Grid.Row="2"
                        Grid.Column="1"
                        Header="ΔYC3:"
                        InputScope="Number"
                        Tag="[ΔYC3]"
                        Text="{Binding Path=SecondTabViewModel.YC3Box.Value, Mode=TwoWay}" />
                    <TextBox
                        Name="yc4"
                        Grid.Row="3"
                        Grid.Column="1"
                        Header="ΔYC4:"
                        InputScope="Number"
                        Tag="[ΔYC4]"
                        Text="{Binding Path=SecondTabViewModel.YC4Box.Value, Mode=TwoWay}" />
                    <TextBox
                        Name="xbc1"
                        Grid.Row="0"
                        Grid.Column="2"
                        Header="ΔXBC1:"
                        InputScope="Number"
                        Tag="[ΔXBC1]"
