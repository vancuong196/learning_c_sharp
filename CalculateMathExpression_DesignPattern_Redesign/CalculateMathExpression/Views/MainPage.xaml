                            Grid.Row="3"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Content}"
                            Content="-"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[SUB]}"
                            Tag="SUB" />
                        <Button
                            Grid.Row="2"
                            Grid.Column="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Content}"
                            Content="2"
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[TWO]}"
                            Tag="TWO" />
                        <Button
                            Grid.Row="4"
                            Grid.Column="2"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Content}"
                            Content="("
                            IsEnabled="{Binding Path=PermissiontabViewModel.Permissons[OPENB]}"
                            Tag="OPENB" />
                        <Button
                            Grid.Row="3"
                            Grid.Column="2"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Command="{Binding Main.ButtonClickedCommand}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Content}"
                            Content="*"
